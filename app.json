{
  "expo": {
    "entryPoint": "./src/App.js",
    "name": "Budget app",
    "slug": "budget-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
